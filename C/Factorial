#include<stdio.h>
void fact(int n);
int main(){
int n;
printf("Enter number whose factorial you want to calculate:\t");
scanf("%d",&n);
fact(n);
printf("Factorial of given number is : %d",factorial);
return 0;
}
void fact(int n){
  int i;
  int factorial = 1;
  for(i = 1 ; i <= n ; i++){
      factorial = factorial * i;
  }
}

